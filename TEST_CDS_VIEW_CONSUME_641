    lo_disp = cl_salv_gui_table_ida=>create(
      iv_table_name = 'ZVCSAE2_CDS_641' " SQL view name from CDS annotation
    ).

    lo_disp->fullscreen( )->display( ).

  CATCH cx_salv_db_connection INTO DATA(lx1).
    MESSAGE lx1->get_text( ) TYPE 'E'.
  CATCH cx_salv_db_table_not_supported INTO DATA(lx2).
    MESSAGE lx2->get_text( ) TYPE 'E'.
  CATCH cx_salv_ida_contract_violation INTO DATA(lx3).
    MESSAGE lx3->get_text( ) TYPE 'E'.
ENDTRY.

